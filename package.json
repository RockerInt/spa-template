{
  "name": "spa-template",
  "version": "1.0.0",
  "description": "Single Page Application Template",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:vendors && yarn build:app",
    "build:vendors": "webpack --config build/webpack.config.vendors.ts",
    "build:app": "webpack --config build/webpack.config.ts",
    "update-api": "nswag swagger2tsclient /Input:http://localhost:5000/api/$spec /Output:src/app/api/ApiClient.generated.ts TypeScriptVersion:2.0 /Template:Fetch /WrapDtoExceptions:true /TypeStyle:Interface /GenerateClientInterfaces:true /DateTimeType:MomentJS"
  },
  "author": "MOCODING, LLC",
  "license": "MIT",
  "dependencies": {
    "applicationinsights-js": "1.0.14",
    "autobind-decorator": "2.1.0",
    "bootstrap": "4.0.0-beta",
    "browser-assert": "1.2.1",
    "moment": "2.20.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-transition-group": "2.2.1",
    "reactstrap": "5.0.0-alpha.4",
    "redux": "3.7.2",
    "redux-automata": "2.1.1",
    "redux-logger": "3.0.6"
  },
  "devDependencies": {
    "@types/applicationinsights-js": "1.0.5",
    "@types/extract-text-webpack-plugin": "3.0.0",
    "@types/react": "16.0.31",
    "@types/react-dom": "16.0.3",
    "@types/react-helmet": "5.0.3",
    "@types/react-hot-loader": "3.0.5",
    "@types/react-redux": "5.0.14",
    "@types/react-router-dom": "4.2.3",
    "@types/react-router-redux": "5.0.10",
    "@types/reactstrap": "5.0.6",
    "@types/redux-logger": "3.0.5",
    "@types/webpack": "3.8.1",
    "@types/webpack-env": "1.13.3",
    "aspnet-prerendering": "3.0.1",
    "aspnet-webpack": "2.0.1",
    "aspnet-webpack-react": "3.0.0",
    "node-noop": "1.0.0",
    "nswag": "11.12.16",
    "react-hot-loader": "3.1.3",
    "ts-node": "4.0.2",
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "typescript": "2.6.2",
    "webpack-hot-middleware": "2.21.0",
    "webpack-typescript-builder": "1.2.5"
  }
}
