    "@types/react-helmet": "5.0.6",
    "@types/react-hot-loader": "3.0.6",
    "@types/react-redux": "5.0.19",
    "@types/react-router-dom": "4.2.6",
    "@types/react-router-redux": "5.0.13",
    "@types/reactstrap": "5.0.24",
    "@types/redux-logger": "3.0.6",
    "@types/webpack": "4.1.4",
