{
  "name": "spa-template",
  "version": "1.0.0",
  "description": "Single Page Application Template",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:vendors && npm run build:app",
    "build:vendors": "webpack --config build/webpack.config.vendors.ts",
    "build:app": "webpack --config build/webpack.config.ts",
    "update-api": "nswag swagger2tsclient /Input:http://localhost:5000/api/$spec /Output:src/app/api/ApiClient.generated.ts TypeScriptVersion:2.0 /Template:Fetch /WrapDtoExceptions:true /TypeStyle:Interface /GenerateClientInterfaces:true /DateTimeType:MomentJS"
  },
  "author": "MOCODING, LLC",
  "license": "MIT",
  "dependencies": {    
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.1.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.1.2",
    "react-router-redux": "5.0.0-alpha.6",   
    "redux": "3.7.2",    
    "redux-logger": "3.0.6",    
    "react-transition-group": "2.2.0",
    "reactstrap": "5.0.0-alpha.2",    
    "bootstrap": "4.0.0-beta",
    "browser-assert": "1.2.1",    
    "moment": "2.18.1",
    "applicationinsights-js": "1.0.12",
    "redux-automata": "2.0.0-beta5"
  },
  "devDependencies": {
    "@types/applicationinsights-js": "1.0.4",
    "@types/extract-text-webpack-plugin": "2.1.0",
    "@types/react": "16.0.7",
    "@types/react-dom": "15.5.5",
    "@types/react-helmet": "5.0.3",
    "@types/react-hot-loader": "3.0.3",
    "@types/react-redux": "4.4.47",
    "@types/react-router-dom": "4.0.7",
    "@types/react-router-redux": "5.0.4",
    "@types/reactstrap": "4.3.9",
    "@types/redux-logger": "3.0.0",
    "@types/webpack": "3.0.9",
    "@types/webpack-env": "1.13.0",
    "aspnet-prerendering": "3.0.1",
    "aspnet-webpack": "2.0.1",
    "aspnet-webpack-react": "3.0.0",
    "autobind-decorator": "2.1.0",
    "node-noop": "1.0.0",
    "nswag": "11.3.4",
    "react-hot-loader": "3.0.0-beta.7",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "tslint-react": "3.2.0",
    "typescript": "2.4.2",
    "webpack-hot-middleware": "2.18.2",
    "webpack-typescript-builder": "1.0.6"
  }
}
